= render 'layouts/page_header', headline: t('activerecord.models.user.other')

%div.content
  %table.table.table-striped
    %thead
      %tr
        %th= t('activerecord.attributes.user.username')
        %th= t('activerecord.attributes.user.email')
        %th{ colspan: "3" }
    %tbody
      - @users.each do |user|
        %tr
          %td= user.username
          %td= user.email
%br/
